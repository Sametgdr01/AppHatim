{
  "name": "apphatim",
  "version": "1.0.0",
  "description": "Hatim takip uygulaması",
  "private": true,
  "workspaces": [
    "mobile",
    "backend"
  ],
  "scripts": {
    "mobile": "cd mobile && npm start",
    "backend": "cd backend && npm start",
    "dev": "concurrently \"npm run mobile\" \"npm run backend\"",
    "install:all": "npm install && cd mobile && npm install && cd ../backend && npm install"
  },
  "keywords": [
    "hatim",
    "quran",
    "mobile",
    "app"
  ],
  "author": "Samet Güder",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
